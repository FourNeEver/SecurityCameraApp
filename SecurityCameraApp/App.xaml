<?xml version = "1.0" encoding = "UTF-8" ?>
<Application xmlns="http://schemas.microsoft.com/dotnet/2021/maui"
             xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
             xmlns:local="clr-namespace:SecurityCameraApp"
             x:Class="SecurityCameraApp.App">
    <Application.Resources>
        <ResourceDictionary>
            <FileImageSource
                x:Key="Arrow">arrow.png
             </FileImageSource>
                <Image x:Key="Frame"
                   Source="nofeed.png"></Image>
                <x:String x:Key="Adress"
                >192.168.1.71</x:String>
            <x:String x:Key="VPort">1234</x:String>
            <x:String x:Key="CPort">1234</x:String>
            <x:String x:Key="TR1">0</x:String>
            <x:String x:Key="MS1">1</x:String>
            <x:String x:Key="RS1">1</x:String>
            <x:String x:Key="TR2">0</x:String>
            <x:String x:Key="MS2">1</x:String>
            <x:String x:Key="RS2">1</x:String>
            <ResourceDictionary.MergedDictionaries>
                <ResourceDictionary Source="Resources/Styles/Colors.xaml" />
                <ResourceDictionary Source="Resources/Styles/Styles.xaml" />
            </ResourceDictionary.MergedDictionaries>
        </ResourceDictionary>
    </Application.Resources>
</Application>
